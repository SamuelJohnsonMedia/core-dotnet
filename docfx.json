{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/Core/Core.csproj",
            "src/Job/Runly.csproj"
          ]
        }
      ],
      "dest": "docs/_meta",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "docs/api/**.yml"
        ]
      }
    ],
    "dest": "docs/_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}